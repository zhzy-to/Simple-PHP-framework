{
  "require": {
    "php": "^5.6",
    "guzzlehttp/guzzle": "^6.5",
    "illuminate/cache": "^5.4",
    "illuminate/filesystem": "^5.4",
    "ext-simplexml": "*",
    "ext-json": "*",
    "illuminate/database": "^5.4",
    "twig/twig": "^1.42",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-zip": "*",
    "symfony/http-foundation": "^3.4"
  },
  "require-dev": {
    "symfony/var-dumper": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "./app/helper.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
